apiVersion: apps/v1
kind: Deployment
metadata:
  labels:
    app: hari-deploy
  name: hari-deploy
spec:
  replicas: 2
  selector:
    matchLabels:
      app: hari-deploy
  template:
    metadata:
      labels:
        app: hari-deploy
    spec:
      containers:
      - image: nginx:latest
        name: nginx
