apiVersion: apps/v1
kind: Deployment
metadata:
  labels:
    app: hari-deploy
  name: hari-deploy
spec:
  replicas: 2
  selector:
    matchLabels:
      app: hari-deploy
  template:
    metadata:
      labels:
        app: hari-deploy
    spec:
      containers:
      - image: nginx:latest
        name: nginx


export NAME=devopsworld.guru
export KOPS_STATE_STORE=s3://kops-hari
export AWS_REGION=us-east-1
export CLUSTER_NAME=devopsworld.guru
